{
	"name": "TheRevenantStar/twig-debugbar",
	"type": "library",
	"description": "A Twig extension to provide access to PHP Debug Bar's rendering functions.",
	"keywords": ["twig", "extension", "php debug bar"],
	"license": "MIT",
	"authors": [
		{
			"name": "Samuel Inman <smtgr15@gmail.com>"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"twig/twig": "^2.4",
		"maximebf/debugbar": "^1.15"

	},
	"replace": {
    "dougfelton/twig-debugbar":"*",
		"bearlikelion/twig-debugbar":"*",
		"smtgr14/twig-debugbar":"*"
	},
	"conflict": {
    "dougfelton/twig-debugbar":"*",
		"bearlikelion/twig-debugbar":"*",
		"smtgr14/twig-debugbar":"*"
	},
	"autoload": {
		"files": ["Extension.php"]
	}
}
